{
  "productName": "Layout Uploader",
  "version": "0.1.0",
  "identifier": "com.layoutuploader.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Layout Uploader",
        "width": 800,
        "height": 600,
        "minWidth": 600,
        "minHeight": 450,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}